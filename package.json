{
  "name": "pokemon-cards-backup",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.1.2",
    "@testing-library/user-event": "^12.2.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express-https-redirect": "^1.0.0",
    "history": "^5.0.0",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.15",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "query-string": "^6.13.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "redux": "^4.0.5",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "dev:server": "cross-env nodemon server/index.js",
    "dev:client": "BROWSER=none react-scripts start",
    "dev": "npm-run-all -p dev:client dev:server",
    "prebuild": "rimraf build",
    "build:client": "react-scripts build",
    "build:server": "./node_modules/webpack/bin/webpack.js --config ./server.webpack.config.js",
    "build": "npm-run-all build:client build:server",
    "start": "node ./build/server.bundle.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/lodash": "^4.14.165",
    "@types/mongoose": "^5.10.1",
    "@types/morgan": "^1.9.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-cli": "^4.2.0"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "src",
      "*.test.js"
    ],
    "watch": [
      "server"
    ],
    "env": {
      "NODE_ENV": "development",
      "PORT": 8000
    },
    "ext": ".*",
    "exec": "babel-node --presets babel-preset-env"
  }
}
